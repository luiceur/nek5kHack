         IF ( (NDIM.EQ.2 .AND. IDIFF.EQ.2)   .OR.
     $        (NDIM.EQ.3 .AND. IDIFF.EQ.6) ) THEN
              IFLMSC(IFLD)         = .TRUE.
              IFMSCR(ICR,IEL,IFLD) = .TRUE.
         ENDIF
 300  CONTINUE
      CALL GLLOG(IFLMSC(IFLD),.TRUE.)
C     
      RETURN
      END
