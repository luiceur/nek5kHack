     $             ,WXM3(LX3), WYM3(LY3), WZM3(LZ3), W3M3(LX3,LY3,LZ3)
     $             ,WAM1(LY1), WAM2(LY2), WAM3(LY3)
     $             ,W2AM1(LX1,LY1), W2CM1(LX1,LY1)
     $             ,W2AM2(LX2,LY2), W2CM2(LX2,LY2)
     $             ,W2AM3(LX3,LY3), W2CM3(LX3,LY3)
# 100 "/lustre/atlas/scratch/csep22/trn001/nek5_acc/nek_2013_11_27/subs2.f" 2
# 100 "/lustre/atlas/scratch/csep22/trn001/nek5_acc/nek_2013_11_27/subs2.f"
      COMMON /SCRMG/ DUMMY6(LX1,LY1,LZ1,LELT,3)
     $             , WA(LX1,LY1,LZ1,LELT)
