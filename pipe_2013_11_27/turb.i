      CALL BCTWALL (V1,V2,V3)
      CALL COPY    (VISKIN,VDIFF(1,1,1,1,IFLD),NTOT1)
      CALL INVCOL2 (VISKIN,VTRANS(1,1,1,1,IFLD),NTOT1)
      CALL SUB2    (VISKIN,VTURB,NTOT1)
      VISMIN = GLMIN(VISKIN,NTOT1)
