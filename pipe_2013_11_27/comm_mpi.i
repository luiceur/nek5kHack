      PID = 0
      NULLPID=0
      NODE0=0
      NODE= NID+1
      
      if (nid.eq.0) then 
         write(6,*) 'Number of processors:',np
         WRITE(6,*) 'REAL    wdsize      :',WDSIZE
         WRITE(6,*) 'INTEGER wdsize      :',ISIZE
      endif
      
      call crystal_setup(cr_h,nekcomm,np)  ! set cr handle to new instan
      
      return
      end
