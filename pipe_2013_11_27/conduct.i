      integer idum,e
      save idum
      data idum /-3/
      
      if (idum.lt.0) return
c     
C     
      mtot = nx1*ny1*nz1*nelv
      if (nx1.gt.8.or.nelv.gt.16) return
      xmin = glmin(x,mtot)
      xmax = glmax(x,mtot)
