     $ ,              H2     (LX1,LY1,LZ1,LELT)
      COMMON /CPRINT/ IFPRINT
      LOGICAL         IFPRINT
C     
      NTOT = NX1*NY1*NZ1*NELT
      CALL SUB3 (DELTAT(1,1,1,1),T(1,1,1,1,IFIELD-1),
     $                           TLAG(1,1,1,1,1,IFIELD-1),NTOT)
      CALL NORMSC (DVNNH1,DVNNSM,DVNNL2,DVNNL8,DELTAT,IMESH)
      INTYPE = -1
