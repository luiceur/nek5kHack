c     $                     IFEXTR(IFIELD) = .TRUE.
      IF (USREL.LT.EXFAC)  IFEXTR(IFIELD) = .TRUE.
      IF(NID.EQ.0 .AND. IFPRINT) 
     $WRITE (6,*) 'Tau, Txnext ',IFIELD,tauss(ifield),txnext(ifield)
C     
      IFSTST(IFIELD) = .FALSE.
      USLIM = 2.*TOLHT(IFIELD)
      IF (USNORM.LT.USLIM) IFSTST(IFIELD) = .TRUE.
C     
      RETURN
      END
