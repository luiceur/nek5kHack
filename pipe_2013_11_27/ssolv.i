      CALL SETTOLT
      IF (NID.EQ.0 .AND. IFPRINT) 
     $WRITE (6,*) 'USNORM, TOLHT',USNORM,TOLHT(IFIELD)
      USREL = USNORM/TOLHT(IFIELD)
