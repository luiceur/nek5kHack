# 854
      FUNCTION VLSC3_acc(X,Y,B,N)
C     
C     local inner product, with weight
C     
      DIMENSION X(n),Y(n),B(n)
      REAL DT
C     

# 1 "/lustre/atlas/scratch/csep22/trn001/nek5_acc/nek_2013_11_27/OPCTR" 1
C     OPCTR is a set of arrays for tracking the number of operations,
C     and number of calls for a particular subroutine
      
# 4
      PARAMETER (MAXRTS=1000)
      COMMON /OPCTRC/ rname(MAXRTS)
      character*6     rname
C     
      COMMON /OPCTRD/ dct(MAXRTS),rct(MAXRTS),dcount
      real*8 dcount,dct,rct
C     
      COMMON /OPCTRI/ ncall(MAXRTS),nrout
C     
      integer myrout,isclld
      save    myrout,isclld
      data    myrout /0/
      data    isclld /0/
# 862 "/lustre/atlas/scratch/csep22/trn001/nek5_acc/nek_2013_11_27/navier4.f" 2
# 862 "/lustre/atlas/scratch/csep22/trn001/nek5_acc/nek_2013_11_27/navier4.f"
C     
      if (isclld.eq.0) then
          isclld=1
          nrout=nrout+1
          myrout=nrout
          rname(myrout) = 'VLSC3 '
      endif
      isbcnt = 3*n
      dct(myrout) = dct(myrout) + dfloat(isbcnt)
      ncall(myrout) = ncall(myrout) + 1
      dcount      =      dcount + dfloat(isbcnt)
      
C     
      DT = 0.0
!$ACC DATA PRESENT(X,Y,B)
!$ACC PARALLEL LOOP REDUCTION(+:DT)
      DO 10 I=1,N
         DT = DT + X(I)*Y(I)*B(I)
 10   CONTINUE
!$ACC END PARALLEL LOOP
!$ACC END DATA
      T=DT
      VLSC3_ACC = T
      
      RETURN
      END
      
