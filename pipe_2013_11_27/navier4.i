      COMMON /TSTEP5/ lyap(3,lpert)  !  lyapunov simulation history
      real lyap
# 689 "/lustre/atlas/scratch/csep22/trn001/nek5_acc/nek_2013_11_27/navier4.f" 2
# 689 "/lustre/atlas/scratch/csep22/trn001/nek5_acc/nek_2013_11_27/navier4.f"
c     
c     
      parameter  (lt=lx1*ly1*lz1*lelt)
      real approx(lt,0:1),h1(1),h2(1),vml(1),vmk(1),ws(1)
      integer napprox(2)
      character*4 name4
c     
      logical ifupdate
      logical ifnewdt
      save    ifnewdt
      data    ifnewdt /.false./
c     
      character*4 name_old
      save    name_old
      data    name_old/'DMIR'/
c     
      real    dtold
      save    dtold
      data    dtold/0.0/
c     
c     First, we have to decide if the dt has changed.
c     
      ifupdate = .false.
      if (dt.ne.dtold) then
         dtold    = dt
         name_old = name4
         ifnewdt  = .true.
         ifupdate = .true.
      elseif (ifnewdt) then
         if (name4.eq.name_old) then
            ifnewdt = .false.
         else
            ifupdate = .true.
         endif
      endif
      if (ifvarp(ifield)) ifupdate = .true.
      if (iflomach)       ifupdate = .true.
      
      if (ifupdate) then    ! reorthogonalize 
         n_sav = napprox(2)
         l     = 1
         do k=1,n_sav
c           Orthogonalize kth vector against {v_1,...,v_k-1}
            if (k.ne.l) then
               ntot = nx1*ny1*nz1*nelfld(ifield)
               call copy(approx(1,l),approx(1,k),ntot)
            endif
            call hconj(approx,l,h1,h2,vml,vmk,ws,name4,ierr)
            if (ierr.eq.0) l=l+1
         enddo
         napprox(2)=min(l,n_sav)
      endif
c     
      return
      end
